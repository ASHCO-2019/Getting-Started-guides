{
  "name": "com.adobe.guides.react",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lorem-ipsum": "^1.0.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "package:clean": "shx rm -rf dist/**/*",
